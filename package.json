{
  "name": "vite-svg-vue",
  "version": "0.0.7",
  "description": "Vite plugin to inline SVG files with Vue.js and automatically optimize them with SVGO",
  "keywords": [
    "vite",
    "vue",
    "svg",
    "laravel",
    "svgo"
  ],
  "main": "./index.js",
  "types": "./index.d.ts",
  "dependencies": {
    "@vue/compiler-sfc": "^3.2.20",
